{
  "name": "tlm-mosaico",
  "productName": "TLM Mosaico",
  "version": "1.0.18",
  "description": "TLM Mosaico - Sistema de Gerenciamento de Mosaicos",
  "main": "dist/main.js",
  "scripts": {
    "start": "electron .",
    "start:debug": "cross-env NODE_OPTIONS=\"--inspect=5858\" electron .",
    "start:debug-brk": "cross-env NODE_OPTIONS=\"--inspect-brk=5859\" electron .",
    "start:debug-watcher": "cross-env NODE_OPTIONS=\"--inspect-brk=5859\" node test-watcher.js",
    "build": "webpack --config webpack.renderer.config.js && webpack --config webpack.main.config.js",
    "dist": "npm run build && electron-builder",
    "dist:win": "npm run build && electron-builder --win",
    "dist:win-unpacked": "npm run build && electron-builder --win --dir",
    "publish": "npm run build && electron-builder --publish=always",
    "publish:github": "npm run build && electron-builder --publish=always --win",
    "update:patch": "node scripts/publish-update.js patch",
    "update:minor": "node scripts/publish-update.js minor",
    "update:major": "node scripts/publish-update.js major",
    "check-updates": "node scripts/publish-update.js check"
  },
  "keywords": [],
  "author": {
    "name": "TlmMosaico",
    "email": "mosaico@tlm.com.br"
  },
  "license": "MIT",
  "build": {
    "appId": "com.tlm.mosaico",
    "productName": "TLM Mosaico",
    "directories": {
      "output": "build-output",
      "buildResources": "build"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "extraResources": [
      {
        "from": "public/iconesGerais",
        "to": "iconesGerais"
      },
      {
        "from": "public/locales",
        "to": "locales"
      }
    ],
    "win": {
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ],

      "requestedExecutionLevel": "asInvoker"
    },
    "nsis": {
      "oneClick": false,
      "allowToChangeInstallationDirectory": true,
      "createDesktopShortcut": true,
      "createStartMenuShortcut": true,
      "shortcutName": "TLM Mosaico",
      "runAfterFinish": true,
      "artifactName": "TLM-Mosaico-Setup-${version}.${ext}",
      "uninstallDisplayName": "TLM Mosaico",
      "deleteAppDataOnUninstall": false
    },
    "publish": [
      {
        "provider": "github",
        "owner": "GabrielNatan2001",
        "repo": "Electron_mosaico",
        "private": false,
        "releaseType": "release",
        "vPrefixedTagName": true,
        "publishAutoUpdate": true
      }
    ],
    "buildDependenciesFromSource": true,
    "npmRebuild": false,
    "compression": "maximum",
    "removePackageScripts": false,
    "removePackageKeywords": false
  },
  "devDependencies": {
    "@babel/core": "^7.28.3",
    "@babel/preset-env": "^7.28.3",
    "@babel/preset-react": "^7.27.1",
    "@svgr/webpack": "^8.1.0",
    "@tailwindcss/postcss": "^4.1.12",
    "@vercel/webpack-asset-relocator-loader": "^1.7.3",
    "autoprefixer": "^10.4.21",
    "babel-loader": "^10.0.0",
    "copy-webpack-plugin": "^13.0.1",
    "cross-env": "^10.0.0",
    "css-loader": "^6.11.0",
    "dotenv": "^17.2.1",
    "electron": "37.3.1",
    "electron-builder": "^26.0.12",
    "node-loader": "^2.1.0",
    "postcss": "^8.5.6",
    "postcss-loader": "^8.1.1",
    "style-loader": "^3.3.4",
    "tailwindcss": "^4.1.12",
    "webpack-cli": "^6.0.1"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.2.1",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-label": "^2.1.7",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-slot": "^1.2.3",
    "axios": "^1.11.0",
    "chokidar": "^4.0.3",
    "class-variance-authority": "^0.7.1",
    "date-fns": "^4.1.0",
    "electron-squirrel-startup": "^1.0.1",
    "electron-updater": "^6.6.2",
    "framer-motion": "^12.23.12",
    "i18next": "^25.4.0",
    "i18next-browser-languagedetector": "^8.2.0",
    "i18next-http-backend": "^3.0.2",
    "lucide-react": "^0.540.0",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "react-hook-form": "^7.62.0",
    "react-i18next": "^15.7.1",
    "react-router-dom": "^7.8.1",
    "react-toastify": "^11.0.5",
    "react-youtube": "^10.1.0",
    "reactflow": "^11.11.4",
    "recharts": "^3.1.2",
    "tailwind-merge": "^3.3.1",
    "tw-animate-css": "^1.3.7",
    "use-media": "^1.5.0",
    "yup": "^1.7.0"
  }
}
